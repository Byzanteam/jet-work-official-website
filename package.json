{
  "name": "official-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue --fix src",
    "setup:git-actions": "degit git@github.com:Byzanteam/jet-scaffolds/git-actions .github/workflows",
    "setup:husky": "degit git@github.com:Byzanteam/jet-scaffolds/husky .husky",
    "setup": "pnpm setup:git-actions --force && pnpm setup:husky --force",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "commitlint": "commitlint"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "i18next": "^22.0.2",
    "lodash-es": "^4.17.21",
    "tailwind-scrollbar": "^2.0.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@byzanteam/commitlint-config": "^1.0.0",
    "@commitlint/cli": "^17.3.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "vite": "^3.1.0"
  }
}
